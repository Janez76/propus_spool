# Database Configuration
# PostgreSQL example: postgresql://user:password@localhost:5432/propus_spool
# SQLite example: sqlite:///./propus_spool.db
DATABASE_URL=sqlite:///./propus_spool.db

# Spoolman Integration
SPOOLMAN_URL=http://localhost:7912
SPOOLMAN_API_KEY=

# Feature Flags
WRITE_MODE=false
PUSH_REMAINING_TO_SPOOLMAN=true
ENABLE_KLIPPER=false

# Application Settings
API_V1_PREFIX=/api/v1
LOG_LEVEL=INFO
